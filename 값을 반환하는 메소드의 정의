
public class HelloWorld {

	public static void main(String[] args) {
		int r=4;
		double radius=getRadius(r);
		double area=getArea(r);
		System.out.printf("둘레 : %f\n",radius);
		System.out.printf("넓이 : %f\n",area);
	}
	
	public static double getRadius(int n) {
		return 3.14*2*n;
	}
	
	public static double getArea(int n) {
		return 3.14*n*n;
	}
	
}
