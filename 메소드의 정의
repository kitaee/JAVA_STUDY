
public class HelloWorld {

	public static void main(String[] args) {
		Calculate(25,5);
	}
	
	public static void Calculate(int a, int b) {
		System.out.printf("%d + %d = %d\n",a,b,a+b);
		System.out.printf("%d - %d = %d\n",a,b,a-b);
		System.out.printf("%d * %d = %d\n",a,b,a*b);
		System.out.printf("%d / %d = %d\n",a,b,a/b);
		System.out.printf("%d mod %d = %d\n",a,b,a%b);
	}

}
